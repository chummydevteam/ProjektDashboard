    <color name="system_primary_color">@*common:color/theme_color</color>
    <color name="system_primary_color_translucent">#90000000</color>
    <color name="system_secondary_color">@*common:color/theme_color_qs_header</color>
    <color name="system_accent_color">@*common:color/theme_color_accent_dark</color>

    <!--Bliss Gerrit ID: I8ccefddf5eb2d38297f385a7ea83752162a7c563-->
    <!--QS Background-->
    <color name="qs_background_drawable_color">@color/system_primary_background</color>

    <!--Quick Settings in Notification drawer -->
    <color name="qs_detail_empty">#24ffffff</color>
    <color name="qs_detail_transition">#66ffffff</color>
    <color name="qs_tile_text">@*common:color/theme_color_qs_tile_text</color>

    <!--All notification popups listed here-->
