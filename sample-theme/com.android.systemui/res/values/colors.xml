        @*common:color/theme_color_notification_background
    </color>
    <color name="notification_material_background_low_priority_color">
        @*common:color/theme_color_notification_background
    </color>
    <color name="notification_material_background_media_default_color">
        @*common:color/theme_color_notification_background
    </color>
    <color name="notification_guts_bg_color">@*common:color/theme_color_notification_background
    </color>
    <color name="notification_guts_title_color">@*common:color/theme_color_notification_primary
